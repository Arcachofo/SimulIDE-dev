rules: ByteCount Address Type Checksum

Checksum-style: default #F0F0F0 true false
Checksum: "[0-9a-fA-F]{2}$"

Type-style: #AA0000 default false false
Type: "^:.{6}([0-9a-fA-F]{2})"

Address-style: #0000AA default true false
Address: "^:.{3}([0-9a-fA-F]{3})"

ByteCount-style: default #FFFFCC false false
ByteCount: "^:[0-9a-fA-F]{2}"


